<svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="100" cy="100" r="95" fill="#0a0a1a" stroke="url(#borderGradient)" stroke-width="2"/>
  
  <!-- Definitions -->
  <defs>
    <!-- Gradients -->
    <linearGradient id="borderGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff66ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#6495ed;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffd700;stop-opacity:1" />
    </linearGradient>
    
    <radialGradient id="galacticGlow" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#ff99ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ff66ff;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#cc33cc;stop-opacity:0.3" />
    </radialGradient>
    
    <radialGradient id="sunGlow" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#ffff99;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ffd700;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#ff9900;stop-opacity:0.4" />
    </radialGradient>
    
    <radialGradient id="earthGlow" cx="40%" cy="40%">
      <stop offset="0%" style="stop-color:#6495ed;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#4169e1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e3a8a;stop-opacity:1" />
    </radialGradient>
    
    <!-- Glow filters -->
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <filter id="strongGlow" x="-100%" y="-100%" width="300%" height="300%">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Stars background -->
  <circle cx="25" cy="30" r="0.8" fill="#ffffff" opacity="0.8"/>
  <circle cx="175" cy="45" r="1" fill="#ffffff" opacity="0.9"/>
  <circle cx="160" cy="170" r="0.7" fill="#ffffff" opacity="0.7"/>
  <circle cx="30" cy="160" r="0.9" fill="#ffffff" opacity="0.8"/>
  <circle cx="140" cy="25" r="0.6" fill="#ffffff" opacity="0.6"/>
  <circle cx="50" cy="180" r="1.2" fill="#ffffff" opacity="0.9"/>
  <circle cx="180" cy="120" r="0.8" fill="#ffffff" opacity="0.7"/>
  <circle cx="20" cy="100" r="0.7" fill="#ffffff" opacity="0.8"/>
  
  <!-- Orbital paths -->
  <circle cx="100" cy="100" r="65" fill="none" stroke="#ff66ff" stroke-width="0.8" opacity="0.3" stroke-dasharray="3,3"/>
  <circle cx="130" cy="85" r="18" fill="none" stroke="#ffd700" stroke-width="0.6" opacity="0.4" stroke-dasharray="2,2"/>
  <circle cx="145" cy="80" r="7" fill="none" stroke="#6495ed" stroke-width="0.5" opacity="0.4" stroke-dasharray="1,1"/>
  
  <!-- Galactic Center (purple) -->
  <circle cx="100" cy="100" r="16" fill="url(#galacticGlow)" filter="url(#strongGlow)"/>
  <circle cx="100" cy="100" r="10" fill="#ff66ff"/>
  <circle cx="97" cy="97" r="3" fill="#ffaaff" opacity="0.8"/>
  
  <!-- Sun orbital position -->
  <circle cx="130" cy="85" r="14" fill="url(#sunGlow)" filter="url(#glow)"/>
  <circle cx="130" cy="85" r="9" fill="#ffd700"/>
  <circle cx="128" cy="83" r="2" fill="#ffff99" opacity="0.9"/>
  
  <!-- Earth -->
  <circle cx="145" cy="80" r="6" fill="url(#earthGlow)" filter="url(#glow)"/>
  <circle cx="145" cy="80" r="5" fill="#4169e1"/>
  <!-- Continents -->
  <ellipse cx="143" cy="79" rx="1.5" ry="1.2" fill="#22c55e" opacity="0.8"/>
  <circle cx="146" cy="81" r="1" fill="#22c55e" opacity="0.8"/>
  
  <!-- Moon -->
  <circle cx="152" cy="77" r="2" fill="#c0c0c0" filter="url(#glow)"/>
  <circle cx="151.5" cy="76.8" r="0.5" fill="#909090" opacity="0.7"/>
  
  <!-- Motion trail effects -->
  <path d="M 165 100 Q 150 105 130 85" stroke="#ffd700" stroke-width="1.5" fill="none" opacity="0.2" stroke-linecap="round"/>
  <path d="M 150 75 Q 148 78 145 80" stroke="#6495ed" stroke-width="1" fill="none" opacity="0.3" stroke-linecap="round"/>
  
  <!-- Speed indicator lines (like motion) -->
  <line x1="155" y1="75" x2="160" y2="73" stroke="#ffffff" stroke-width="1" opacity="0.4"/>
  <line x1="156" y1="77" x2="162" y2="75" stroke="#ffffff" stroke-width="0.8" opacity="0.3"/>
  <line x1="154" y1="79" x2="159" y2="78" stroke="#ffffff" stroke-width="0.6" opacity="0.2"/>
  
  <!-- App Name Text -->
  <text x="100" y="195" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="url(#borderGradient)" text-anchor="middle">ORBITAL SIM</text>
</svg>